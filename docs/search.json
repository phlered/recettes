---
layout: null
---
[
{% assign pages = site.pages | where_exp:'p','p.name != "index.md" and p.name != "search.html"' %}
{% for page in pages %}
  {
    "title": "{{ page.title | escape }}",
    "url": "{{ page.url | relative_url }}",
    "content": "{{ page.content | strip_html | replace: '\n', ' ' | escape }}",
    "tags": "{{ page.tags | join: ', ' }}"
  }{% unless forloop.last %},{% endunless %}
{% endfor %}
]
